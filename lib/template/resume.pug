// "resume" type
// Author: Kevin Cyu

// Using list format in doc type
include mixin/doc-list.pug
// Main
doctype html
html
    // Include head
    include head/doc-head.pug
    body
        div.container
            div.paper
                // Paper Title
                div(id = "top" class = "demo-title")
                    div.row.flex-center
                        div.text-center 
                            h1 #{title}
                // Section tag as button
                div(id = "nav")
                    include body/resume-nav.pug
                // Section body 
                include body/resume-body.pug
                div(class = "to-top")
                    a(href = "#top" class="paper-btn margin") Top
                // Include copyright
                include footer/copy.pug